<main x-data="SeoAnalyzer">
  <div class="prose max-w-3xl mx-auto">
    <h1>SEO Analyzer</h1>
    <p class="mt-0">Comprehensive SEO analysis for your website. Paste a full URL, run the analyzer, and get a prioritized list of improvements.</p>

    <form @submit.prevent class="space-y-5">
      <div>
        <label for="targetUrl">Page URL</label>
        <input id="targetUrl" type="url" x-model="url" placeholder="https://example.com" />
      </div>

      <fieldset>
        <legend class="text-sm font-semibold">Modules</legend>
        <div class="flex flex-wrap gap-4 mt-2">
          <label class="inline-flex items-center gap-2 text-sm">
            <input type="checkbox" x-model="options.checkSemantics" />
            <span>Semantics</span>
          </label>
          <label class="inline-flex items-center gap-2 text-sm">
            <input type="checkbox" x-model="options.checkMeta" />
            <span>Meta tags</span>
          </label>
          <label class="inline-flex items-center gap-2 text-sm">
            <input type="checkbox" x-model="options.checkHeadings" />
            <span>Headings</span>
          </label>
          <label class="inline-flex items-center gap-2 text-sm">
            <input type="checkbox" x-model="options.checkSchemas" />
            <span>Schemas</span>
          </label>
          <label class="inline-flex items-center gap-2 text-sm">
            <input type="checkbox" x-model="options.checkAccessibility" />
            <span>Accessibility</span>
          </label>
        </div>
      </fieldset>

      <div class="flex items-center gap-4">
        <button @click="analyze" type="submit" class="text-bold leading-4">
          <span x-text="buttonLabel">Analyze SEO</span>
        </button>

        <div class="cursor-pointer font-mono text-sm px-3 py-1.5 rounded-full transition-all bg-gray-100 hover:bg-gray-200 border-2 border-gray-300 inline-flex gap-2 items-center text-terminal"
             x-show="state==='success'" x-cloak
             @click="reset">
          {% phicon "trash", "regular", { class: "size-5" } %}
          <span class="hidden xs:inline">reset</span>
        </div>
      </div>

      <p class="text-red-600 text-sm md:text-base mt-2" x-show="state==='error' && errorMessage" x-text="String(errorMessage)" x-cloak></p>
    </form>
  </div>

  <div class="prose max-w-3xl mx-auto mt-8" x-show="state!=='idle'" x-cloak>
    <template x-if="state==='loading'">
      <div class="flex items-center gap-3 text-sm md:text-base">
        <div class="size-6 rounded-full border-2 border-orange-300 border-r-transparent animate-spin"></div>
        <span>Analyzing page…</span>
      </div>
    </template>

    <template x-if="state==='success'">
      <div>
        <h2 class="mt-0">Score</h2>
        <div class="not-prose border rounded-xl p-4 flex items-center justify-between gap-4"
             :class="scoreCardClass">
          <div class="min-w-0">
            <div class="text-sm opacity-70">Overall SEO score</div>
            <div class="text-2xl font-semibold" x-text="(result && result.score ? result.score : 0) + '/100'"></div>
            <div class="text-xs opacity-70 mt-1">
              <span class="font-mono" x-text="result && result.url ? result.url : ''"></span>
              · <span x-text="analyzedAtLocal"></span>
            </div>
          </div>
          <div class="shrink-0">
            <div class="size-14 rounded-full grid place-items-center font-mono text-lg border" :class="scoreBadgeClass">
              <span x-text="result && typeof result.score==='number' ? result.score : 0"></span>
            </div>
          </div>
        </div>

        <template x-if="result && result.recommendations && result.recommendations.length">
          <div class="mt-8">
            <h3>Recommendations</h3>
            <ol class="not-prose list-none p-0 m-0 space-y-2">
              <template x-for="rec in sortedRecommendations" :key="rec.id">
                <li class="border rounded-xl overflow-hidden ml-0 pl-0">
                  <div class="flex items-start justify-between gap-4 p-3">
                    <div class="min-w-0">
                      <div class="flex justify-between items-center">
                        <div class="flex flex-wrap items-center gap-2">
                          <span class="text-xs px-2 py-0.5 rounded-full border" :class="severityBadge(rec.severity)" x-text="severityLabel(rec.severity)"></span>
                          <span class="text-xs px-2 py-0.5 rounded-full bg-gray-100 border text-gray-700" x-text="rec.category"></span>
                        </div>
                        <button class="text-right text-xs sm:text-sm" @click="toggleOpen(rec.id)" x-show="hasRecDetails(rec)" x-cloak :aria-expanded="openIndex.includes(rec.id)">
                          <span x-text="openIndex.includes(rec.id)?`Hide`:`Show`" x-cloak>Show</span> code example
                        </button>
                      </div>
                      <div class="font-semibold mt-1" x-text="rec.title"></div>
                      <div class="text-sm opacity-80" x-text="rec.description"></div>
                      <div class="text-xs mt-1 opacity-70" x-show="rec.impact" x-text="'Impact: ' + rec.impact"></div>
                    </div>
                  </div>
                  <div class="" x-show="openIndex.includes(rec.id) && hasRecDetails(rec)" x-collapse x-cloak>
                    <template x-if="rec.code">
                      <div class="relative grid mt-3">
                        <pre class="text-xs overflow-auto rounded-none p-3 my-0"><code class="language-html" x-html="escapeCode(rec.code)"></code></pre>
                        <button class="flex items-center gap-1 text-xs font-mono text-cursor hover:text-yellow-400 bg-terminal border border-current rounded-full px-2 py-1 absolute z-10 top-2.5 right-2"
                                @click="copy(rec.code)">
                          {% phicon "copy", "duotone", { class: "size-3" } %}
                          <span>Copy<span class="sr-only"> code example</span></span>
                        </button>
                      </div>
                    </template>
                  </div>
                </li>
              </template>
            </ol>
          </div>
        </template>

        <div class="mt-10">
          <h3>Detailed analysis</h3>
          <div class="not-prose grid gap-4">
            <!-- Semantics -->
            <section class="border rounded-xl overflow-hidden p-4">
              <h4 class="text-lg m-0 flex items-center gap-2">{% phicon "tree-structure", "regular", { class: "size-5" } %} <span>Semantics</span></h4>
              <div class="text-sm mt-2 grid grid-cols-1 sm:grid-cols-2 gap-2">
                <div>main:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.semantics && result.semantics.hasMain)"></strong> (<span x-text="result && result.semantics ? (result.semantics.mainCount || 0) : 0"></span>)</span></div>
                <div>header:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.semantics && result.semantics.hasHeader)"></strong></span></div>
                <div>footer:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.semantics && result.semantics.hasFooter)"></strong></span></div>
                <div>nav:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.semantics && result.semantics.hasNav)"></strong></span></div>
                <div>articles:&nbsp;&nbsp;<span class="font-mono"><strong x-text="result && result.semantics ? (result.semantics.articleCount || 0) : 0"></strong></span></div>
                <div>sections:&nbsp;&nbsp;<span class="font-mono"><strong x-text="result && result.semantics ? (result.semantics.sectionCount || 0) : 0"></strong></span></div>
                <div>asides:&nbsp;&nbsp;<span class="font-mono"><strong x-text="result && result.semantics ? (result.semantics.asideCount || 0) : 0"></strong></span></div>
                <div>divs:&nbsp;&nbsp;<span class="font-mono"><strong x-text="result && result.semantics ? (result.semantics.divCount || 0) : 0"></strong></span></div>
              </div>
              <template x-if="result && result.semantics && result.semantics.issues && result.semantics.issues.length">
                <div class="pt-3">
                  <div class="text-sm font-semibold">Issues</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="i in result.semantics.issues">
                      <li class="text-red-700" x-text="i"></li>
                    </template>
                  </ul>
                </div>
              </template>
              <template x-if="result && result.semantics && result.semantics.warnings && result.semantics.warnings.length">
                <div class="pt-2">
                  <div class="text-sm font-semibold">Warnings</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="w in result.semantics.warnings">
                      <li class="text-yellow-800" x-text="w"></li>
                    </template>
                  </ul>
                </div>
              </template>
            </section>

            <!-- Meta -->
            <section class="border rounded-xl overflow-hidden p-4">
              <h4 class="text-lg m-0 flex items-center gap-2">{% phicon "meta-logo", "regular", { class: "size-5" } %} <span>Meta tags</span></h4>
              <div class="text-sm mt-2 grid grid-cols-1 sm:grid-cols-2 gap-2">
                <div>Title:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasTitle)"></strong> (<span x-text="result && result.meta ? (result.meta.titleLength || 0) : 0"></span> chars)</span></div>
                <div>Description:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasMetaDescription)"></strong> (<span x-text="result && result.meta ? (result.meta.metaDescriptionLength || 0) : 0"></span> chars)</span></div>
                <div>Viewport:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasViewport)"></strong></span></div>
                <div>Charset:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasCharset)"></strong></span></div>
                <div>Lang:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasLang)"></strong> (<span x-text="result && result.meta && result.meta.lang ? result.meta.lang : '-' "></span>)</span></div>
                <div>Canonical:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasCanonical)"></strong></span></div>
                <div>OG tags:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasOpenGraph)"></strong></span></div>
                <div>Twitter Card:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasTwitterCard)"></strong></span></div>
                <div>Robots:&nbsp;&nbsp;<span class="font-mono"><strong x-text="bool(result && result.meta && result.meta.hasRobots)"></strong> (<span x-text="robotsFlags"></span>)</span></div>
              </div>
              <template x-if="result && result.meta && result.meta.issues && result.meta.issues.length">
                <div class="pt-3">
                  <div class="text-sm font-semibold">Issues</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="i in result.meta.issues">
                      <li class="text-red-700" x-text="i"></li>
                    </template>
                  </ul>
                </div>
              </template>
              <template x-if="result && result.meta && result.meta.warnings && result.meta.warnings.length">
                <div class="pt-2">
                  <div class="text-sm font-semibold">Warnings</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="w in result.meta.warnings">
                      <li class="text-yellow-800" x-text="w"></li>
                    </template>
                  </ul>
                </div>
              </template>

              <div class="pt-3 text-right">
                <a class="btn btn--secondary btn--xs"
                   :href="buildToolLink('metadata-checker')"
                   x-show="encodedUrl()" x-cloak
                   target="_blank"
                   aria-label="Open Metadata Checker for this URL">
                  {% phicon "meta-logo", "regular", { class: "size-4" } %}
                  <span>Open Metadata Checker</span>
                </a>
              </div>
            </section>

            <!-- Headings -->
            <section class="border rounded-xl overflow-hidden p-4">
              <h4 class="text-lg m-0 flex items-center gap-2">{% phicon "article-ny-times", "regular", { class: "size-5" } %} <span>Headings</span></h4>
              <div class="text-sm mt-2">
                <div>H1 count: <strong x-text="result && result.headings ? (result.headings.h1Count || 0) : 0"></strong></div>
                <template x-if="result && result.headings && result.headings.structure && result.headings.structure.length">
                  <div class="pt-2">
                    <div class="text-sm font-semibold mb-2">Structure</div>
                    <ol class="not-prose list-none p-0 m-0 space-y-1 md:space-y-2">
                      <template x-for="h in result.headings.structure">
                        <li class="flex items-start gap-2">
                          <span class="shrink-0 inline-block leading-0" :style="`padding-left: ${(h.level - 1) * 0.75}rem`">
                            <span class="inline-flex items-center justify-center h-5.5 px-2 rounded-md border bg-white border-orange-200 text-xs font-mono font-bold">H<span x-text="h.level"></span></span>
                          </span>
                          <div class="grow mt-0.5 text-sm md:mt-0 md:text-base leading-snug" x-text="h.text"></div>
                        </li>
                      </template>
                    </ol>
                  </div>
                </template>
                <div class="pt-3 text-right">
                  <a class="btn btn--secondary btn--xs"
                     :href="buildToolLink('heading-extractor')"
                     x-show="encodedUrl()" x-cloak
                     target="_blank"
                     aria-label="Open Heading Extractor for this URL">
                    {% phicon "article-ny-times", "regular", { class: "size-4" } %}
                    <span>Open Heading Extractor</span>
                  </a>
                </div>
              </div>
            </section>

            <!-- Schemas -->
            <section class="border rounded-xl overflow-hidden">
              <h4 class="text-lg m-0 pt-4 px-4 flex items-center gap-2">{% phicon "brackets-curly", "regular", { class: "size-5" } %} <span>Schemas</span></h4>
              <div class="text-sm mt-2">
                <div class="flex flex-wrap gap-2 px-4" x-show="result && result.schemas && result.schemas.detected && result.schemas.detected.length">
                  <div class="text-sm font-semibold w-full shrink-0">Detected</div>
                  <template x-for="s in result.schemas.detected">
                    <span class="text-xs px-2 py-0.5 rounded-full border border-green-200 bg-green-50 text-green-800" x-text="s"></span>
                  </template>
                </div>
                <template x-if="missingSchemasList && missingSchemasList.length">
                  <div class="pt-2 px-4">
                    <div class="text-sm font-semibold">Missing schemas (with suggestions)</div>
                    <ol class="not-prose list-none p-0 m-0 mt-2">
                      <template x-for="item in missingSchemasList">
                        <li class="border rounded-lg p-0 overflow-hidden mb-2">
                          <div class="flex items-center justify-between gap-3 p-3">
                            <div class="min-w-0">
                              <div class="flex flex-wrap items-center gap-2">
                                <span class="text-xs px-2 py-0.5 rounded-full bg-yellow-50 border border-yellow-200 text-yellow-800" x-text="item.name"></span>
                                <template x-if="item.schemaUrl">
                                  <a :href="item.schemaUrl" target="_blank" rel="nofollow noopener" class="text-xs">Schema.org</a>
                                </template>
                              </div>
                            </div>
                            <button class="text-right text-xs sm:text-sm" x-show="hasSchemaExample(item)" x-cloak @click="toggleOpen(missingKey(item.name))" :aria-expanded="openIndex.includes(missingKey(item.name))">
                              <span x-text="openIndex.includes(missingKey(item.name))?`Hide`:`Show`" x-cloak>Show</span> JSON‑LD example
                            </button>
                          </div>
                          <div class="grid relative" x-show="openIndex.includes(missingKey(item.name)) && hasSchemaExample(item)" x-collapse x-cloak>
                            <pre class="text-xs overflow-auto rounded-none p-3 m-0"><code class="language-json" x-html="escapeCode(jsonLdExamplePretty(item))"></code></pre>
                            <button class="flex items-center gap-1 text-xs font-mono text-cursor hover:text-yellow-400 bg-terminal border border-current rounded-full px-2 py-1 absolute z-10 top-2.5 right-2"
                                    @click="copy(jsonLdExamplePretty(item))">
                              {% phicon "copy", "duotone", { class: "size-3" } %}
                              <span>Copy<span class="sr-only"> JSON‑LD example</span></span>
                            </button>
                          </div>
                        </li>
                      </template>
                    </ol>
                  </div>
                </template>
                <div class="p-3 text-right">
                  <a class="btn btn--secondary btn--xs"
                     href="https://developers.google.com/search/docs/appearance/structured-data?utm_source=freshjuice.dev"
                     target="_blank"
                     rel="nofollow noopener"
                     aria-label="Open Schema Markup Testing Tool">
                    {% phicon "code", "regular", { class: "size-4" } %}
                    <span>Open Schema Markup Testing Tool</span>
                  </a>
                </div>
              </div>
            </section>

            <!-- Accessibility -->
            <section class="border rounded-xl p-4">
              <h4 class="text-lg m-0 flex items-center gap-2">{% phicon "person-arms-spread", "regular", { class: "size-5" } %} <span>Accessibility</span></h4>
              <div class="text-sm mt-2 grid grid-cols-1 sm:grid-cols-2 gap-2 font-mono">
                <div>Images without alt: <strong x-text="result && result.accessibility ? (result.accessibility.imagesWithoutAlt || 0) : 0"></strong> / <span x-text="result && result.accessibility ? (result.accessibility.totalImages || 0) : 0"></span></div>
                <div>Images with empty alt: <strong x-text="result && result.accessibility ? (result.accessibility.imagesWithEmptyAlt || 0) : 0"></strong></div>
                <div>Links without text: <strong x-text="result && result.accessibility ? (result.accessibility.linksWithoutText || 0) : 0"></strong> / <span x-text="result && result.accessibility ? (result.accessibility.totalLinks || 0) : 0"></span></div>
                <div>Inputs without labels: <strong x-text="result && result.accessibility ? (result.accessibility.inputsWithoutLabels || 0) : 0"></strong> / <span x-text="result && result.accessibility ? (result.accessibility.totalInputs || 0) : 0"></span></div>
                <div>Buttons without text: <strong x-text="result && result.accessibility ? (result.accessibility.buttonsWithoutText || 0) : 0"></strong> / <span x-text="result && result.accessibility ? (result.accessibility.totalButtons || 0) : 0"></span></div>
                <div>Skip link present: <strong x-text="bool(result && result.accessibility && result.accessibility.hasSkipLink)"></strong></div>
              </div>
              <template x-if="result && result.accessibility && result.accessibility.issues && result.accessibility.issues.length">
                <div class="pt-3">
                  <div class="text-sm font-semibold">Issues</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="i in result.accessibility.issues">
                      <li class="text-red-700" x-text="i"></li>
                    </template>
                  </ul>
                </div>
              </template>
              <template x-if="result && result.accessibility && result.accessibility.warnings && result.accessibility.warnings.length">
                <div class="pt-2">
                  <div class="text-sm font-semibold">Warnings</div>
                  <ul class="list-disc pl-6 text-sm font-mono my-2">
                    <template x-for="w in result.accessibility.warnings">
                      <li class="text-yellow-800" x-text="w"></li>
                    </template>
                  </ul>
                </div>
              </template>
            </section>
          </div>
        </div>
      </div>
    </template>
  </div>

  <article class="prose max-w-3xl mx-auto mt-12">
    <h2>About this tool</h2>
    <p>
